#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int t;
    scanf("%d", &t);
    double m;
    if(t <= 800){
        m = t * 0.9;
    }
    else if(t > 800 && t < 1500){
        m = t * 0.9 * 0.9;
    }
    else{
        m = t * 0.9 * 0.79;
    }
    printf("%.1f\n", m);



	return 0;
}
