#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    int max = 0;
    if (a > max) {
        max = a;
    }

    if (b > max) {
        max = b;
    }

    if (c > max) {
        max = c;
    }

    printf("%d\n", max);



	return 0;
}
