#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int n; 
    double w; 

    
    scanf("%d", &n);


    for (int i = 0; i < n; i++) {
        scanf("%lf", &w); 
        double area = w * w; 
        area = round(area * 10) / 10.0; //四捨五入到整數再退回去
        printf("%.1f\n", area); 
    }




	return 0;
}
