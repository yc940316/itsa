#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    char s[10];
    char *en[] = {"dog", "cat", "duck", "cow", "fox"};
    char *ch[] = {"狗", "貓", "鴨", "牛", "狐"};
    scanf ("%s", s);
    for (int i = 0; i < 5; i++) {
        if (strcmp(s, en[i]) == 0) {
            printf("%s\n", ch[i]);
            return 0;
        }
    }
     for (int i = 0; i < 5; i++) {
        if (strcmp(s, ch[i]) == 0) {
            printf("%s\n", en[i]);
            return 0;
        }
    }




	return 0;
}
