#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int n;
    scanf("%d", &n);
    int b = 0;
    for (int i = 2; i * i < n; i++) {
        if(n % i == 0) {
            printf("NO\n");
            b = 1;
            break;
        }
    }
    if(b == 0){
        printf("YES\n");
    }




	return 0;
}
