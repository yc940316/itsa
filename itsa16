#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int n;
    scanf("%d", &n);
    for(int times = 0; times < n; times++) {
        int i;
        int ans = 1;
        scanf ("%d", &i);
        if (i > 31) {
            printf("Value of more than 31\n");
            continue;
        }
        for (int e = 0; e < i; e++) {
            ans = ans * 2;
        }
        printf("%d\n", ans);
    }




	return 0;
}
