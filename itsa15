#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int p;
    scanf("%d", &p);
    if (p < 10) {
        printf("%d\n", p * 100);
    }
    else if (p >= 10 && p <30) {
        printf("%d\n", p * 90);
    }
    else if (p >= 30 && p < 100) {
        printf("%d\n", p * 80);
    }
    else {
        printf("%d\n", p * 70);
    }


	return 0;
}
