#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

   int n;
   scanf("%d", &n);
   int bmi[n];
   for(int i = 0; i < n; i++) {
        scanf("%d", &bmi[i]);
   }
   for(int i = 0; i < n; i++) {
        if(bmi[i] >= 28){
        printf("肥胖\n");
        }
        if(bmi[i] < 28 && bmi[i] >= 24){
        printf("體重過重\n");
        }
        if(bmi[i] < 24 && bmi[i] >= 18.5){
        printf("正常\n");
        }
        if(bmi[i] < 18.5){
        printf("體重過輕\n");
        }
   }
