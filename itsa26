#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

    int a, b, c;
    int f = 0;
    scanf("%d %d %d", &a, &b, &c);
    if(a+b>c){
        if(b+c>a){
            if(a+c>b){
                printf("fit\n");
                f = 1;
            }
        }
    }
    if(f==0){
        printf("unfit\n");
    }



	return 0;
}
